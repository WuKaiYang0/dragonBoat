{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.fixAll.ts": "always"
  },
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml"
  ],

  "editor.formatOnSave": true,
  "rest-client.environmentVariables": {
    "$shared": {},
    "dev": {
      "url": "http://dragonboat.scnu.edu.cn"
    }
  }
}
